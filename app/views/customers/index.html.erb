<h2>Customers</h2>

<ul>
  <% @customers.each do |customer| %>
    <li><%= link_to customer.email, "https://#{@shop_session.domain}/admin/customers/#{customer.id}", target: "_top" %></li>
  <% end %>
</ul>

<hr>
<%= form_tag import_customers_path, multipart: true do %>
  <%= file_field_tag :file %>
  <%= submit_tag 'Submit'%>
<% end %>